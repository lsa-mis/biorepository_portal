<div class="d-flex flex-row align-items-center justify-content-between">
  <h1>Import Data Report</h1>
  <%= link_to 'Back to Reports', reports_path, class: "btn btn-outline-primary m-3 z-3 shadow" %>
</div>
<p>This report shows Import Data Logs statistics.</p>

<div class="py-3">
  <%= render 'filter_form', report_url: import_data_report_reports_path %>

  <% if params[:commit] && params[:format] == 'html' %>
    <%= render 'reports_table' %>
  <% end %>
</div>
